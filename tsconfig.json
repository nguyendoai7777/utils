
{
    "compilerOptions": {
        "incremental": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "strict": true,
        "noImplicitReturns": true,
        "moduleResolution": "node",
        "stripInternal": true,
        "noEmit": true,
        "lib": [
            "esnext",
            "dom"
        ],
        "target": "esnext",
        "baseUrl": ".",
        "paths": {
            "@utils": ["./src/index"],
            "@utils/operators": ["./src/operators/index"]
        }
    }
}